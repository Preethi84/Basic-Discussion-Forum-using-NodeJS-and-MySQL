          <div class="container mt-4">
          <div class="card-body ">
            <blockquote class="blockquote mb-0">
              <% if(userData.length){ 
                                    
                for(var i = 0;i < userData.length;i++) { %>
                      <a href="/home/display/<%= userData[i].post_id %>">
                        <%=userData[i].post_id %>
                       <%=userData[i].Question %></a>
                       <a href="/home/delete/<%=userData[i].post_id%>" > <img class="del" src="/p.png" width="50" height="50"  ></a>                     
                       &nbsp;&nbsp;&nbsp; 
                       <a href="/home/edit/<%=userData[i].post_id%>"> <img class="del" src="/e.png" width="50" height="50" ></a>                     
                                        
                         <h6 class="namemve">Posted by <%=userData[i].Name %></h6></p><hr>
                         <% }
                
                        }else{ %>
                            <tr>
                               <td colspan="3">No Questions have been posted by <%=userData[i].Name %> </td>
                            </tr>
                       <% } %>
                           
                  </blockquote>
              </div>
          </div>
              
        
